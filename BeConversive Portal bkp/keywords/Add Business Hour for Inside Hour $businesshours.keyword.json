        "isRawScript": true,
        "rawScript": "$currentDate \u003d new Date();\n//$currentDate \u003d new Date(\"Wed Feb 28 2024 11:15:12 GMT+0530 (India Standard Time)\")\n\nvar $fromTime \u003d  $currentDate.getHours();\nvar $toTime \u003d $fromTime+1;\n_log($fromTime);\n_log($toTime)\n\nfunction time($time){\n\tvar $amORpm;\n\tvar $setTime;\n\t\n\tif($time\u003e\u003d12 \u0026\u0026 $time\u003c\u003d23)\n\t{\n\t\t$time \u003d $time - 12;\n\t\t$amORpm \u003d \"pm\"\n\t}\n\telse{\n\t\tif($time\u003e\u003d24){\n\t\t\t$time \u003d $time-24;\n\t\t}\n\t\telse{\n\t\t\t$time \u003d $time\n\t\t}\n\t\t$amORpm \u003d \"am\"\n\t}\n\n\tif($time\u003c10){\n\t\t$setTime\u003d \"0\"+$time.toString();\n\t}\n\telse{\n\t\t$setTime \u003d $time.toString();\n\t}\n\tvar $arr \u003d [];\n\t$arr.push($amORpm);\n\t$arr.push($setTime)\n\treturn $arr;\n}\n\nvar $amORpmFrom \u003d time($fromTime)[0]\n_log($amORpmFrom)\nvar $amORpmTo \u003d time($toTime)[0]\n_log($amORpmTo)\nvar $from \u003d time($fromTime)[1]\n_log($from)\nvar $to \u003d time($toTime)[1]\n_log($to)"
