        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"control\", _near(_heading3(\"Enable Login Access for SMS-Magic support team\")))"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Disabled\", _near(_paragraph(\"info-text\")))"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Disabled\""
        }
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "}\nelse\n{"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Disabled\", _near(_paragraph(\"info-text\")))"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Disabled\""
        }
      },
