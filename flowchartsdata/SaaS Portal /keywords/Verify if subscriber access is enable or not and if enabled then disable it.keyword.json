        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Default access period is 7 days. After 7 days this access gets disabled automatically.\""
        }
      },
      {
        "commented": false,
        "action": "_isVisible",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"control\", _near(_heading3(\"Enable Login Access for SMS-Magic support team\")))"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": false,
        "action": "_isVisible",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Enabled\", _near(_paragraph(\"Default access period is 7 days. After 7 days this access gets disabled automatically.\")))"
          }
        ],
        "stepType": "Store",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "$stateSubscriberAccess"
        }
      },
      {
        "commented": false,
        "action": "_log",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "$stateSubscriberAccess"
          }
        ],
        "stepType": "Action"
      },
