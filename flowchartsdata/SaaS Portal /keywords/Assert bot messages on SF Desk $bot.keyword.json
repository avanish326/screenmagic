        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_button(\"All\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"slds-float_left senderName\", _near(_button(\"All\")))"
