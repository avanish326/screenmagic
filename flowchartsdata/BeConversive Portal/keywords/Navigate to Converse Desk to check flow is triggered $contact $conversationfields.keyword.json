          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_paragraph(\"Bot Conversations\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"name-txt\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "var $phoneNumber1 \u003d $phoneNumber;\n$formattedNumber \u003d $phoneNumber1.replace(/\\D/g, \"\");\n\n_log($formattedNumber);\n"
      },
      {
        "commented": false,
        "action": "_containsText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_heading1(0)"
          },
          {
            "isCustomParameter": true,
            "customValue": "$formattedNumber"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "var $senderID1\u003d$conversationfields.senderID;\n\n\nvar $inputString \u003d$senderID1;\nvar $parts \u003d $inputString.split(\u0027 \u0027);\nvar $senderID \u003d $parts[0].trim();\n_log($senderID);\n"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_label(\"\"+$senderID+\"\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "$senderID"
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_paragraph(\"Chatbot is active for this conversation. Click here to disable the Bot and take over the conversation\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Chatbot is active for this conversation. Click here to disable the Bot and take over the conversation\""
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_paragraph(\"chatbot_triggered\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Hi there! Thank you for contacting The BrightCare Hospital secure chat. If this is an emergency, Please call 911 immediately.\""
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_paragraph(\"chatbot_triggered[1]\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"What can we help you with? Please choose one of the following options: Type 1 for Find doctor Type 2 for Book Appointment Type 3 for Health packages Type 4 for Location\""
        }
