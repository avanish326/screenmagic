      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_div(\"select-dropdown-indicator\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "$time \u003d _collectAttributes(\"_span\",\"/.*/\",\"sahiText\",_in(_div(\"/menu-list/\")))\n$times \u003d [];\nfor(var $i \u003d 0;$i\u003c$time.length;$i++){\nif($time[$i]!\u003d\"2 Minutes\"){\n$times.push($time[$i])\n}\n}\n$selectRandom \u003d _random($times.length - 2)\n$el \u003d $times[$selectRandom]\n\n\n\n$conversion \u003d $el.toLowerCase();\n_log($conversion);\n"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span($el)"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": true,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"45 Minutes\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_submit(\"Save\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_div(\"notification-content no-child\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Your custom inactivity logout duration has been updated.\""
        }
      }
    ]
