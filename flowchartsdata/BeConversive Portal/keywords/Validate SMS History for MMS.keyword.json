    "parameters": [],
    "steps": [
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_div(\"Analytics\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Message History\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_wait",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "10000"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_cell(3, _near(_tableHeader(\"Message\")))"
          }
        ],
        "stepType": "Store",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "$messageTextInHistory"
        }
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "_log($messageTextInHistory)\n\n"
      },
      {
        "commented": false,
        "action": "_areEqual",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "$messageTextEntered"
          },
          {
            "isCustomParameter": true,
            "customValue": "$messageTextInHistory"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_link(\"info\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_containsText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_cell(1, _near(_cell(\"Encoding\")))"
          },
          {
            "isCustomParameter": true,
            "customValue": "\"MMS\""
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      }
    ]
