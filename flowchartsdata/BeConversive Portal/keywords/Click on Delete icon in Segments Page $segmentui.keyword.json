{
  "modifiedOn": "2024-08-27T05:10:59.645Z",
  "name": "Click on Delete icon in Segments Page $segmentui",
  "id": "58264",
  "type": 3,
  "createdOn": "2024-07-10T13:53:28.580Z",
  "content": {
    "isSystemKeyword": false,
    "title": "Click on Delete icon in Segments Page $segmentui",
    "parameters": [
      {
        "parameterId": "segmentui_",
        "entityTypeId": "segmentui",
        "suffix": ""
      }
    ],
    "steps": [
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_svg_path(2, _near(_cell(\"\"+$segmentui.editedSegmentName+\" copy 1\")))"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_isVisible",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_div(\"modal-content\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": true,
        "action": "_isVisible",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_image(\"icon\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": false,
        "action": "_isVisible",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_svg_rect(0)"
          }
        ],
        "stepType": "Assert",
        "assertType": "Is_True"
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Delete Segment\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Delete Segment\""
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_paragraph(\"Are you sure you want to delete the segment? This action cannot be undone.\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Are you sure you want to delete the segment? This action cannot be undone.\""
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_submit(\"Cancel\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Cancel\""
        }
      },
      {
        "commented": false,
        "action": "_getText",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_submit(\"Delete\")"
          }
        ],
        "stepType": "Assert",
        "assertType": "Equals",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "\"Delete\""
        }
      }
    ]
  },
  "enabled": false,
  "parentId": ""
}