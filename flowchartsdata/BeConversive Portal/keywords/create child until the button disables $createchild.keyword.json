          "customValue": "$button"
        }
      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "\nif(!_areEqual(\"/cursor-not-allowed/\",$disableCheck)){\n$i \u003d _random($letterOne.length-1);\n$j \u003d _random($letterTwo.length-1);\n$childName \u003d $createchild.accountName+$letterOne[$i]+$letterTwo[$j];\n_click(_submit(\"Create Account\"));\n_setValue(_textbox(\"Account Name\"), $childName);\n_setValue(_emailbox(\"Email\"), $mailOne+$letterOne[$i]+$letterTwo[$j]+$mailTwo);\n_click(_submit(\"Create\"));\n_wait(5000);\n}else{\nbreak;\n}\n}"
