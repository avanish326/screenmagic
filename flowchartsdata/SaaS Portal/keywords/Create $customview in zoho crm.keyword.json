  "content": {
    "title": "Create $customview in zoho crm",
    "parameters": [
      {
        "parameterId": "customview_",
        "entityTypeId": "customview",
        "suffix": ""
      }
    ],
    "steps": [
      {
        "commented": false,
        "action": "_navigateTo",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "\"https://crm.zoho.in\""
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"Leads[1]\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_div(\"lyteDummyEventContainer[2]\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_link(\"New Custom View\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_setValue",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_textbox(\"customviewname\")"
          },
          {
            "isCustomParameter": true,
            "customValue": "\"random\""
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"None\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_span(\"select2-textcondition-container\")"
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_setValue",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_textbox(\"searchValue\")"
          },
          {
            "isCustomParameter": true,
            "customValue": "\"Pune\""
          }
        ],
        "stepType": "Action"
      },
      {
        "commented": false,
        "action": "_click",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_submit(\"Save\")"
          }
        ],
        "stepType": "Action"
      }
    ]
  },
  "enabled": false,
  "parentId": ""
}