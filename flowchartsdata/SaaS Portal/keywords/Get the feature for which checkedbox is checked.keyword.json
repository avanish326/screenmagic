      },
      {
        "commented": false,
        "isRawScript": true,
        "rawScript": "$collectCheckbox \u003d _collect(\"_checkbox\",\"/on/\",_in(_div(\"access-control-wrap\")))\n$info \u003d [];\nfor(var $i \u003d 0; $i\u003c$collectCheckbox.length;$i++){\nif(_isChecked($collectCheckbox[$i])){\n$info.push(_getText(_span(\"/text-span/\",_leftOf(_checkbox(\"on[\"+$i+\"]\"),25))))\n}\n}\n$info \u003d $info.sort();\n\n\n\n"
      },
      {
        "commented": true,
        "action": "_call",
        "stepParameters": [
          {
            "isCustomParameter": true,
            "customValue": "_sahi.blurEnabled\u003dtrue"
          }
        ],
        "stepType": "Store",
        "secondaryParam": {}
      },
      {
        "commented": true,
        "action": "_count",
        "stepParameters": [],
        "stepType": "Store",
        "secondaryParam": {
          "isCustomParameter": true,
          "customValue": "$gridCount"
        }
      },
      {
        "commented": true,
        "isRawScript": true,
        "rawScript": "\n\n$collectInfo \u003d _collectAttributes(\"_link\",\"/.*/\",\"sahiText\",_in(_list(\"nav navbar-nav menuList mr-auto\")))\n\n\n$collectInfo \u003d $collectInfo.sort()\n\n_assertEqual($collectInfo,$info)\n"
      },
      {
        "commented": true,
